package com.emarbox.enums;


public enum PlatformType {

    INFO_UNION(1,"信息流联盟"),
    GDT(2,"广点通"),
    TOU_TIAO(3,"头条");
    private Integer index;
    private String desc;


    PlatformType(Integer index,String desc) {
        this.index = index;
        this.desc = desc;
    }

    public String getDesc() {
        return desc;
    }

    public void setDesc(String desc) {
        this.desc = desc;
    }

    public Integer getIndex() {
        return index;
    }

    public void setIndex(Integer index) {
        this.index = index;
    }
}
